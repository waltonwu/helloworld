CREATE USER PUBUSER
  IDENTIFIED BY pubuser
  DEFAULT TABLESPACE PUB
  TEMPORARY TABLESPACE TEMP
  PROFILE DEFAULT
  ACCOUNT UNLOCK;
  -- 2 Roles for PUBUSER
  GRANT DBA TO PUBUSER;
  GRANT CONNECT TO PUBUSER;
  ALTER USER PUBUSER DEFAULT ROLE ALL;
  -- 1 System Privilege for PUBUSER
  GRANT UNLIMITED TABLESPACE TO PUBUSER;
  -- 2 Tablespace Quotas for PUBUSER
  ALTER USER PUBUSER QUOTA UNLIMITED ON PUB;
  ALTER USER PUBUSER QUOTA UNLIMITED ON PUBIDX;
  ALTER USER PUBUSER QUOTA UNLIMITED ON IMGDATA;
   

CREATE USER wds
  IDENTIFIED BY narisq
  DEFAULT TABLESPACE HPS
  TEMPORARY TABLESPACE TEMP
  PROFILE DEFAULT
  ACCOUNT UNLOCK;
  GRANT DBA TO wds;
  GRANT CONNECT TO wds;
  ALTER USER wds DEFAULT ROLE ALL;
GRANT
  CREATE SESSION, CREATE ANY TABLE, CREATE ANY VIEW ,CREATE ANY INDEX, CREATE ANY PROCEDURE,
  ALTER ANY TABLE, ALTER ANY PROCEDURE,
  DROP ANY TABLE, DROP ANY VIEW, DROP ANY INDEX, DROP ANY PROCEDURE,
  SELECT ANY TABLE, INSERT ANY TABLE, UPDATE ANY TABLE, DELETE ANY TABLE
  TO wds;
  -- 1 System Privilege for PUBUSER
  GRANT UNLIMITED TABLESPACE TO wds;
  ALTER USER wds QUOTA UNLIMITED ON HPS;


CREATE USER nc
  IDENTIFIED BY nariacc
  DEFAULT TABLESPACE USERS
  TEMPORARY TABLESPACE TEMP
  PROFILE DEFAULT
  ACCOUNT UNLOCK;
  GRANT DBA TO nc;
  GRANT CONNECT TO nc;
  ALTER USER nc DEFAULT ROLE ALL;
  GRANT UNLIMITED TABLESPACE TO nc;
  ALTER USER NC QUOTA UNLIMITED ON NARIDATA;
  

impdp wds/narisq@127.0.0.1:1521/ORCL dumpfile=d:\wds.dpdmp

cmd控制台输入
exp wds/narisq@127.0.0.1:1521/ORCL dumpfile=d:\wds.dpdmp
