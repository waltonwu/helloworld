CREATE BIGFILE TABLESPACE IMGDATA DATAFILE
  'D:\app\walton\oradata\imcnn\IMGDATA1' SIZE 1000M AUTOEXTEND ON
LOGGING
ONLINE
PERMANENT
EXTENT MANAGEMENT LOCAL AUTOALLOCATE
BLOCKSIZE 8K
SEGMENT SPACE MANAGEMENT AUTO
FLASHBACK ON;


CREATE BIGFILE TABLESPACE NARIDATA DATAFILE
  'D:\oracle\oradata\imcnn\NARIDATA1' SIZE 1000M AUTOEXTEND ON
LOGGING
ONLINE
PERMANENT
EXTENT MANAGEMENT LOCAL AUTOALLOCATE
BLOCKSIZE 8K
SEGMENT SPACE MANAGEMENT AUTO
FLASHBACK ON;


